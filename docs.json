{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Blockline",
  "colors": {
    "primary": "#60bb06",
    "light": "#60bb06",
    "dark": "#60bb06"
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Overview",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "authentication",
              "rate-limits"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "openapi": "api-reference/openapi.json"
          }
        ]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Use Cases & Patterns",
            "pages": [
              "guides/use-cases",
              "guides/integration-patterns"
            ]
          },
          {
            "group": "Code Examples",
            "pages": [
              "guides/examples/javascript",
              "guides/examples/python"
            ]
          },
          {
            "group": "Advanced",
            "pages": [
              "guides/error-handling",
              "guides/performance"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://blockline.mevseer.com",
          "icon": "chart-line"
        },
        {
          "anchor": "API Status",
          "href": "https://api.soltop.sh/health",
          "icon": "heart-pulse"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Get API Key",
        "href": "https://blockline.mevseer.com/dashboard/api-keys"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://blockline.mevseer.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/soltopsh",
      "github": "https://github.com/soltop"
    }
  }
}
